      <dep package="icu"/>
      <dep package="librevenge"/>
      <dep package="libxml2"/>
      <dep package="zlib"/>
    </dependencies>
  </autotools>

  <!-- OpenJPEG https://github.com/uclouvain/openjpeg -->
  <cmake
    id="openjpeg">
    <branch
      module="uclouvain/openjpeg/archive/v2.3.1.tar.gz"
